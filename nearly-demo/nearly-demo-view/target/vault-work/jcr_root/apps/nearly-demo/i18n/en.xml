<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:language="en"
          jcr:mixinTypes="[mix:language]"
          jcr:primaryType="sling:Folder">
    <locText
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="locText"
        sling:message="Get my location"/>
    <homepageUrl 
		jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="homepageUrl"
        sling:message="/content/en/homepage.html"/>
     <error404Title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="error404Title"
        sling:message="Oops you seem to be lost"
        />
    <backWord
    	jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="backWord"
        sling:message="Back to homepage"
        />
</jcr:root>
